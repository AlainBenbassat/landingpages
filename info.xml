<?xml version="1.0"?>
<extension key="landingpages" type="module">
  <file>landingpages</file>
  <name>Landing Pages</name>
  <description>create landing pages with a header, left column, right column, and footer</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/landingpages</url>
    <url desc="Documentation">https://github.com/AlainBenbassat/landingpages</url>
    <url desc="Support">https://github.com/AlainBenbassat/landingpages</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-06-05</releaseDate>
  <version>1.1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.78</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Landingpages</namespace>
    <format>25.01.1</format>
    <angularModule>crmLandingpages</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Landingpages\AutomaticUpgrader</upgrader>
</extension>
